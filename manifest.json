{
  "name": "Ritual",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Information tracker!",
  "browser_action": {
    "default_icon": "icon_48.png"
  },
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["ritual.js"]
    }
  ]
}
